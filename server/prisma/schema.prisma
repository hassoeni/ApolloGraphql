// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  // url      = env("DATABASE_URL")
  url      = "file:./dev.db"
}

model Track {
  id                Int    @id @default(autoincrement())
  title             String
  thumbnail         String
  length            Int
  modulesCount      Int
  description       String
  numberOfViews     Int 
  durationInSeconds Int
  // later add author 
  // later add module 
}
